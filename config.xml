<?xml version='1.0' encoding='utf-8'?>
<widget
        id="com.phonegap.alarm"
        version="1.0.0"
        xmlns="http://www.w3.org/ns/widgets"
        xmlns:android = "http://schemas.android.com/apk/res/android"
        xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Alarm</name>
    <description>
        Trying alarm app
    </description>
    <author email="tolulopeibiyode@gmail.com" href="#">
        Gods1son
    </author>
    <preference name="orientation" value="portrait"/>
    <preference name="permissions" value="none"/>
    <preference name="KeepRunning" value="false"/>
    <content src="index.html" />
    <plugin name="https://github.com/katzer/cordova-plugin-local-notifications" spec="0.8.5"/>
    <plugin name="cordova-plugin-media" spec="4.0.0"/>
    <gap:plugin name="cordova-plugin-x-socialsharing" source="npm" spec="5.1.6"/>
    <gap:config-file platform="android" parent="/manifest/application">
    <activity android:launchMode="singleInstance" />
</gap:config-file>
</widget>
